{
  "name": "less",
  "description": "Less Renderer for ChiÏ‡",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/less.git"
  },
  "dependencies": {
    "less": "1.x.x"
  },
  "nodes": [
    {
      "name": "render",
      "ns": "less",
      "description": "Less Renderer",
      "phrases": {
        "active": "Rendering LESS"
      },
      "ports": {
        "input": {
          "less": {
            "title": "Less",
            "type": "string",
            "required": true
          }
        },
        "output": {
          "error": {
            "title": "Error",
            "type": "object"
          },
          "css": {
            "title": "CSS",
            "type": "string"
          }
        }
      },
      "dependencies": {
        "npm": {
          "less": "latest"
        }
      },
      "fn": "output = [less, 'render', $.less]\n"
    }
  ]
}